#include <stdio.h> //função para a entrada e saída de dados#include <stdio.h> //função para a entrada e saída de dados#include <stdio.h> //função para a entrada e saída de dados

int main () //função de determina exatamente o que será executado

{
    int x,y;   //regras das variáveis: não inicia com número, n tem acento, n tem caracter especial, tem tem separação de sílaba
            // pode ter underline e faz diferença maiusculo e minusculo
                        //int é um tipo de dado que manipula números inteiros e tem no máximo 4 bytes
    
    printf("Digite dois números:\n");
    scanf("%d",&x);
    scanf("%d",&y);
    printf("x+y=%d\n", x+y);
    
    return 0;
   
}

    scanf("%d",&x);
    scanf("%d",&y);
    printf("x+y=%d\n", x+y);
    
    return 0;
   
}
